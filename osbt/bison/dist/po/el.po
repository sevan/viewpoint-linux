msgid "%@: syntax error: expected %0e before %u"
msgstr "%@: συντακτικό σφάλμα: αναμενόταν %0e πριν από %u"

#: examples/c/bistromathic/parse.y:375
msgid "%@: syntax error: expected %0e or %1e before %u"
