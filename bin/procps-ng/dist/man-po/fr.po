msgstr "B<Q>"

#. type: Plain text
#: ../slabtop.1:77
msgid "Quit the program."
msgstr "Quitter le programme."

#. type: TP
#: ../slabtop.1:78
#, no-wrap
msgid "I</proc/slabinfo>"
msgstr "I</proc/slabinfo>"

#. type: Plain text
