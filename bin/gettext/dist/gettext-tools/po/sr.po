msgstr "      --use-fuzzy             узима у обзир нејасне ставке\n"

#: src/msgcmp.c:244
#, c-format
msgid "      --use-untranslated      consider untranslated entries\n"
msgstr "      --use-untranslated      узима у обзир непреведене ставке\n"

